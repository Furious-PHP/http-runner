{
  "name": "furious/psr15",
  "description": "PHP 7.4 HTTP Runner",
  "license": "MIT",
  "keywords": ["http", "runner", "php", "php 7.4"],
  "authors": [
    {
      "name": "Arslanov Rasul",
      "email": "rasularslanoov@gmail.com"
    }
  ],
  "require": {
    "php": ">=7.4",
    "psr/http-message": "^1.0",
    "psr/http-server-handler": "^1.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^5.4",
    "furious/psr7": "1.3.*"
  },
  "autoload": {
    "psr-4": {
      "Furious\\HttpRunner\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\Furious\\HttpRunner\\": "tests/"
    }
  },
  "scripts": {
    "test": "phpunit --stderr --colors=always"
  }
}